{
    "env": {
        "browser": true,
        "node": true,
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "no-console": "off",
        "react/prop-types": "off"
    },
    "globals": {
        "React": "writable"
    },
    "parser": "babel-eslint"
}
